(require 'powerline)
;(powerline-default-theme)
(powerline-default-theme)

(set-face-attribute 'mode-line nil
                    :foreground "#fff"
                    :background "#458b74"
                    :box nil)

(set-face-attribute 'powerline-active1 nil
                    :foreground "#fff"
                    :background "#66cdaa"
                    :inherit 'mode-line)

(set-face-attribute 'powerline-active2 nil
                    :foreground "#000"
                    :background "#76eec6"
                    :inherit 'mode-line)
